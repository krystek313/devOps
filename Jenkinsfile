pipeline 
{
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'I am building project right now '
            }
        }
            stage('Test') {
            steps {
                echo 'I am testing project right now '
            }
        }   
        stage('Deploy') {
            steps {
                echo 'I am deploying project right now '
            }
        }
    }
    
}
